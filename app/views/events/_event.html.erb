<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Started at:</strong>
    <%= event.started_at %>
  </p>

  <p>
    <strong>Finished at:</strong>
    <%= event.finished_at %>
  </p>

</div>
